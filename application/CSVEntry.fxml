<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="100.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.CSVEntryController">
   <children>
      <TextField fx:id="textFieldCSVEntry" layoutX="27.0" layoutY="50.0" prefHeight="25.0" prefWidth="342.0" promptText="CSV File Path (or name)" />
      <Button fx:id="addCSV" layoutX="401.0" layoutY="50.0" mnemonicParsing="false" onAction="#buttonHandling" prefHeight="25.0" prefWidth="60.0" text="Add" />
      <RadioButton fx:id="itemCSV" layoutX="292.0" layoutY="21.0" mnemonicParsing="false" text="ItemCSV" />
      <RadioButton fx:id="printerCSV" layoutX="401.0" layoutY="21.0" mnemonicParsing="false" text="PrinterCSV" />
      <Label fx:id="csvEntry" layoutX="106.0" layoutY="13.0" prefHeight="27.0" prefWidth="140.0" text="Please enter CSV">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
