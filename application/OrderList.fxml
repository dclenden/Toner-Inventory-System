<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="500.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.orderViewController">
   <children>
      <Button fx:id="returnButton" layoutX="590.0" layoutY="353.0" mnemonicParsing="false" onAction="#buttonHandling" text="return" />
      <TableView fx:id="itemTable" layoutX="14.0" layoutY="44.0" prefHeight="200.0" prefWidth="667.0">
        <columns>
          <TableColumn fx:id="printerModel" prefWidth="101.0" text="printerModel" />
          <TableColumn fx:id="brand" prefWidth="85.0" text="brand" />
            <TableColumn fx:id="model" minWidth="0.0" prefWidth="66.0" text="model" />
            <TableColumn fx:id="numPrinters" minWidth="0.0" prefWidth="61.0" text="# printers" />
            <TableColumn fx:id="minStock" prefWidth="59.0" text="minStock" />
            <TableColumn fx:id="curStock" minWidth="0.0" prefWidth="54.0" text="curStock" />
            <TableColumn fx:id="ordered" minWidth="0.0" prefWidth="72.0" text="onOrder" />
            <TableColumn fx:id="needed" minWidth="0.0" prefWidth="168.0" text="needed" />
        </columns>
      </TableView>
      <Button fx:id="exportButton" layoutX="589.0" layoutY="270.0" mnemonicParsing="false" onAction="#buttonHandling" text="export" />
      <Label fx:id="filePathConfirmation" contentDisplay="CENTER" layoutX="91.0" layoutY="27.0" prefHeight="17.0" prefWidth="474.0" />
   </children>
</AnchorPane>
